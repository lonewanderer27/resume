---
const { profile } = Astro.props as { profile: string };
---

{profile && (
  <section class="career-profile">
    <h2 class="title">Career Profile</h2>
    <div class="details">{profile}</div>
  </section>
)}

<style lang="scss">
.career-profile {
  .title {
    @include header;
  }
  .details {
    @include body-text;
  }
}
</style>